{
  "expectation_suite_name": "experiment_data_suite",
  "meta": {
    "great_expectations_version": "0.18.0",
    "notes": "Data quality expectations for A/B test experiment data"
  },
  "expectations": [
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "user_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "variant"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "experiment_name"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "user_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "variant"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "experiment_name"
      }
    },
    {
      "expectation_type": "expect_compound_columns_to_be_unique",
      "kwargs": {
        "column_list": ["experiment_name", "user_id"]
      },
      "meta": {
        "notes": "Each user should only appear once per experiment"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "variant",
        "value_set": ["control", "variant_a", "variant_b", "variant_c", "treatment"]
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "converted",
        "value_set": [0, 1, true, false, "0", "1", "true", "false"]
      },
      "meta": {
        "notes": "Conversion should be binary"
      }
    },
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 100
      },
      "meta": {
        "notes": "Need at least 100 users for meaningful analysis"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "revenue",
        "min_value": 0,
        "max_value": 100000
      },
      "meta": {
        "notes": "Revenue should be reasonable per user"
      }
    },
    {
      "expectation_type": "expect_column_proportion_of_unique_values_to_be_between",
      "kwargs": {
        "column": "variant",
        "min_value": 0.1,
        "max_value": 1.0
      },
      "meta": {
        "notes": "Should have multiple variants represented"
      }
    }
  ]
}
