{
  "expectation_suite_name": "revenue_data_suite",
  "meta": {
    "great_expectations_version": "0.18.0",
    "notes": "Comprehensive data quality expectations for revenue/transaction uploads"
  },
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_ordered_list",
      "kwargs": {
        "column_list": ["date", "amount", "status"]
      },
      "meta": {
        "notes": "Core required columns for revenue data"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "amount"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "date"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "amount"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "date"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "amount",
        "min_value": 0,
        "max_value": 10000000
      },
      "meta": {
        "notes": "Revenue should be positive and under 10M per transaction"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_of_type",
      "kwargs": {
        "column": "amount",
        "type_": "float64"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "status",
        "value_set": ["completed", "paid", "pending", "refunded", "cancelled", "failed"]
      }
    },
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 1
      },
      "meta": {
        "notes": "Should have at least one row"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_strftime_format",
      "kwargs": {
        "column": "date",
        "strftime_format": "%Y-%m-%d"
      },
      "meta": {
        "notes": "Date should be in YYYY-MM-DD format"
      }
    },
    {
      "expectation_type": "expect_column_median_to_be_between",
      "kwargs": {
        "column": "amount",
        "min_value": 1,
        "max_value": 10000
      },
      "meta": {
        "notes": "Median transaction should be reasonable"
      }
    },
    {
      "expectation_type": "expect_column_proportion_of_unique_values_to_be_between",
      "kwargs": {
        "column": "date",
        "min_value": 0.01,
        "max_value": 1.0
      },
      "meta": {
        "notes": "Should have some variety in transaction dates"
      }
    },
    {
      "expectation_type": "expect_column_kl_divergence_to_be_less_than",
      "kwargs": {
        "column": "status",
        "partition_object": {
          "values": ["completed", "paid"],
          "weights": [0.4, 0.4]
        },
        "threshold": 0.6
      },
      "meta": {
        "notes": "Most transactions should be successful"
      }
    },
    {
      "expectation_type": "expect_column_distinct_values_to_be_in_set",
      "kwargs": {
        "column": "status",
        "value_set": ["completed", "paid", "pending", "refunded", "cancelled", "failed"]
      }
    },
    {
      "expectation_type": "expect_column_value_lengths_to_be_between",
      "kwargs": {
        "column": "status",
        "min_value": 4,
        "max_value": 15
      }
    }
  ]
}
